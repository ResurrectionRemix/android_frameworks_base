<resources>

    <!-- Base attributes available to SlimSeekBarPreference. -->
    <declare-styleable name="SlimSeekBarPreference">
        <attr name="interval" format="integer" />
        <attr name="min" format="integer" />
        <attr name="unitsLeft" format="string|reference" />
        <attr name="unitsRight" format="string|reference" />
    </declare-styleable>

    <!-- Custom Analog clock -->
    <declare-styleable name="CustomAnalogClock">
        <attr name="custom_dial" format="reference"/>
        <attr name="custom_hand_hour" format="reference"/>
        <attr name="custom_hand_minute" format="reference"/>
        <attr name="deadpool_dial" format="reference"/>
        <attr name="deadpool_hand_hour" format="reference"/>
        <attr name="deadpool_hand_minute" format="reference"/>
    </declare-styleable>

</resources>
